class Solution {
    // Function to find the minimum element in a sorted and rotated array.
    findMin(arr) {
        // your code here

        // arr.sort((a,b)=>a-b);
        // return arr[0];
        //tc- O(nlogn) sc-O(n)
        
        let min=arr[0];
        for(let i=0;i<arr.length;i++){
            if(arr[i]<min){
                min=arr[i];
            }
        }
        return min;
       //SC-O(1) tc-O(n)
    }
}
