class Solution {
    //Function to find transpose of a matrix.
    transpose(matrix, n)
    {
        //your code here
        for(let i=0;i<n;i++){
            for(let j=i;j<matrix.length;j++){
                let temp = matrix[i][j];
                matrix[i][j] = matrix[j][i];
                matrix[j][i] = temp;
            }
        }
        return matrix;
    }
}
