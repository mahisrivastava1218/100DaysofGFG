class Solution {
    //Function to check whether a string is reversible or not.
    isReversible(str, n)
    {
        let i=0;
        let j=n-1;
        while(i<j){
           if(str.charAt(i) !== str.charAt(j)){
               return false;
           }
           i++;
           j--;
        }
        return true; //palindrome
    }
    // TC- O(n/2) - O(n)
    // sc - O(1);
}
