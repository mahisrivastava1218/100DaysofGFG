class Solution {
    /**
     * @param number k
     * @param number[] arr
     * @returns number
     */
    search(k, arr) {
        // code here
        //bruite force
           for (let i = 0; i < arr.length; i++) {
          if (arr[i] === k) {
            return i + 1; 
          }
        }
    // If k is not found, return -1
    //TC - O(n) SC - O(1);
    return -1;
    }
}
