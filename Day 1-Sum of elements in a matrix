class Solution {
    sumOfMatrix(N,M,Grid){
       //code here
        let sum = 0;
    //     for(let i=0;i<N;i++){
    //     for(let j=0;j<M;j++){
    //         sum += Grid[i][j];
        
    //     }
    //   }
    //   return sum;
      //concise code
        for(let row of Grid){
              for(let element of row){
                sum += element;
              }
        }
        return sum;
        //TC - O(N*M) , SC - O(1);
    }
    
   
}
